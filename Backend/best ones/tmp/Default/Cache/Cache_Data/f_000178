                paging: true,
                pageLength: 15,
                lengthChange: false,
                info: true,
                responsive: true
            });

            SupervisionDatatable = $('#supervisionListTablePersonalDatatable').DataTable({
                processing: true,
                searching: true,
                sorting: false,
                paging: true,
                pageLength: 15,
                lengthChange: false,
                info: true,
